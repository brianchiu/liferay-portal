{
	"csfConfig": {
		"path:**/*.js": {
			"js": {
				"lint": {
					"globals": {
						"$": true,
						"AlloyEditor": true,
						"CKEDITOR": true,
						"MODULE_PATH": true,
						"_": true
					}
				}
			}
		}
	},
	"dependencies": {
		"metal": "^2.16.5",
		"metal-component": "^2.16.3",
		"metal-debounce": "^2.0.0",
		"metal-dom": "^2.5.18",
		"metal-drag-drop": "^3.1.0",
		"metal-events": "^2.16.5",
		"metal-position": "^2.1.1",
		"metal-soy": "^2.13.2",
		"metal-state": "^2.13.2",
		"metal-uri": "^3.1.0"
	},
	"devDependencies": {
		"babel-cli": "6.26.0",
		"babel-preset-env": "^1.6.1",
		"check-source-formatting": "^2.0.4",
		"cross-env": "^5.1.3",
		"liferay-npm-bundler": "^2.1.0",
		"metal-tools-soy": "^4.0.0",
		"rimraf": "^2.6.2"
	},
	"name": "layout-admin-web",
	"scripts": {
		"build": "metalsoy --soyDeps \"node_modules/+(clay-button|clay-icon|com.liferay.frontend.js.web|com.liferay.frontend.taglib|com.liferay.frontend.taglib.clay|com.liferay.layout.admin.web)/**/*.soy\" && cross-env NODE_ENV=production babel -d classes/META-INF/resources src/main/resources/META-INF/resources && liferay-npm-bundler && npm run cleanSoy",
		"checkFormat": "npm run csf",
		"cleanSoy": "rimraf src/**/*.soy.js",
		"csf": "csf src/**/*.js src/**/*.scss src/**/*.soy",
		"format": "npm run csf -- -i"
	},
	"version": "3.0.0"
}
