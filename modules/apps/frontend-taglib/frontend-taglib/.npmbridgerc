{
	"cards_treeview": {
		"dest-file-mapper": {
			"from": "(.*)\\$(.*)@.*/lib/(.*)",
			"to": "$2/src/$3"
		},
		"dest-mod-name-mapper": {
			"from": "(.*)\\.js$",
			"to": "bridge/cards_treeview/$1"
		},
		"file-globs": "cards_treeview/**/*.js",
		"input": "classes/META-INF/resources",
		"output": "classes/META-INF/resources/bridge/frontend-taglib",
		"src-mod-name-mapper": {
			"from": "(.*)\\.js$",
			"to": "frontend-taglib/$1"
		}
	},
