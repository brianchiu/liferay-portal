{
	"dependencies": {
		"html2canvas": "^1.0.0-alpha.12",
		"metal": "^2.13.2",
		"metal-component": "^2.13.2",
		"metal-debounce": "^2.0.0",
		"metal-dom": "^2.13.2",
		"metal-soy": "^2.13.2",
		"metal-state": "^2.13.2",
		"metal-uri": "^2.3.0"
	},
	"devDependencies": {
		"babel-cli": "6.26.0",
		"babel-preset-env": "^1.6.1",
		"check-source-formatting": "^2.0.4",
		"cross-env": "^5.1.3",
		"liferay-npm-bundler": "^2.1.0",
		"metal-tools-soy": "^4.0.0",
		"rimraf": "^2.6.2"
	},
	"name": "fragment-web",
	"scripts": {
		"build": "metalsoy --soyDeps \"node_modules/+(com.liferay.frontend.js.web)/**/*.soy\" && cross-env NODE_ENV=production babel -d classes/META-INF/resources src/main/resources/META-INF/resources && liferay-npm-bundler && npm run cleanSoy",
		"checkFormat": "npm run csf",
		"cleanSoy": "rimraf src/**/*.soy.js",
		"csf": "csf src/**/*.js src/**/*.scss src/**/*.soy",
		"format": "npm run csf -- -i"
	},
	"version": "2.0.0"
}
